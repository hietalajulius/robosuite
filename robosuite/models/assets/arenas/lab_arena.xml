<mujoco model="lumi_with_table v200">
<compiler meshdir="../meshes"/>
    <asset>
        <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" width="512" height="512"/>
        <material name="MatGnd" reflectance="0.5" texture="texplane" texrepeat="1 1" texuniform="true" />
        <mesh name="table" file="../meshes/table.stl" />
    </asset>



    <worldbody>

        <light diffuse=".8 .8 .8" dir="0 -.15 -1" directional="false" pos="0 0 4.0" specular="0.3 0.3 0.3" />
        <geom name="ground" pos="0 0 0" size="5 5 10" type="plane" contype="1" conaffinity="1" />

        <body name="tabletop" pos="0 0 0">
            <geom type="box" pos="0 0 0" size="0.3 0.3 0.14" group="1" />
        </body>

        <body pos="0.02 -0.105 0.14">
            <composite type="grid" count="9 9 1" spacing="0.02"  >
                <joint kind="main" damping="0.001" stiffness="0.001"/>
                <tendon width="0.001" kind="shear" damping="0.001" stiffness="0.001" /> 
                <!--<skin material="white_borders" texcoord="true" inflate="0.0002" subgrid="2"/>-->
                <geom type="capsule" size="0.005" rgba=".8 .2 .1 1" friction='1.5 1.5 1.5' solref='0.01 1' solimp='0.99 0.99 0.01' condim='4' />
            </composite>
        </body>
        <site name="target0" pos="0 0 0.3" size="0.005 0.005 0.005" rgba="0 0 1 1" type="sphere"></site>
        <site name="target1" pos="0 0 0.4" size="0.005 0.005 0.005" rgba="0 0 1 1" type="sphere"></site>

        <!-- front view -->
        <camera mode="fixed" name="frontview" pos="1.6 0 1.45" quat="0.56 0.43 0.43 0.56"/>
        <!-- bird view -->
        <camera mode="fixed" name="birdview" pos="-0.2 0 3.0" quat="0.7071 0 0 0.7071"/>
        <!-- agent view -->
        <camera mode="fixed" name="agentview" pos="0.5 0 1.35" quat="0.653 0.271 0.271 0.653"/>
        <!-- side view -->
        <camera mode="fixed" name="sideview" pos="-0.05651774593317116 1.2761224129427358 1.4879572214102434" quat="0.009905065491771751 0.006877963156909582 0.5912228352893879 0.806418094001364" />
    </worldbody>

    <equality>
            <distance solref="0.001 1" geom1="G0_8" geom2="gripper0_grip_geom" />
    </equality>

</mujoco>
